{
    "name": "@apathoid/eslint-config",
    "version": "1.1.0",
    "description": "ESLint configuration file.",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/apathoid/eslint-config.git"
    },
    "type": "module",
    "main": "./index.cjs",
    "exports": {
        "import": "./index.js",
        "require": "./index.cjs",
        "default": "./index.cjs"
    },
    "files": [
        "index.js",
        "index.cjs",
        "index.d.ts"
    ],
    "scripts": {
        "build:cjs": "babel index.js -o index.cjs",
        "prepare": "npm run build:cjs"
    },
    "keywords": [
        "eslint",
        "eslintconfig",
        "linter"
    ],
    "author": "apathoid",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/apathoid/eslint-config/issues"
    },
    "homepage": "https://github.com/apathoid/eslint-config#readme",
    "peerDependencies": {
        "@eslint/js": ">=9",
        "@stylistic/eslint-plugin": ">=5",
        "eslint": ">=9",
        "globals": ">=16",
        "typescript-eslint": ">=8"
    },
    "devDependencies": {
        "@babel/cli": "^7.28.3",
        "@babel/core": "^7.28.4",
        "@babel/plugin-transform-modules-commonjs": "^7.27.1"
    }
}
